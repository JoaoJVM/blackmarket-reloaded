{
    "name": "blackmarket-reloaded",
    "version": "0.0.0",
    "dependencies": {
        "requirejs": "latest",
        "angular": "latest",
        "angular-route": "latest",
        "angularAMD": "latest",
        "jquery": "latest",

        "bootswatch-dist": "flatly",
        "ionicons": "latest",
        "components-font-awesome": "latest"
    },
    "install": {
        "base": "lib",
        "path": {
            "js": "{name}/js",
            "css": "{name}/css",
            "eot": "{name}/fonts",
            "svg": "{name}/fonts",
            "ttf": "{name}/fonts",
            "woff": "{name}/fonts",
            "woff2": "{name}/fonts",
            "otf": "{name}/fonts"
        },
        "sources": {
            "requirejs": "bower_components/requirejs/*.js",
            "angular": "bower_components/angular/*.js",
            "angular-route": "bower_components/angular-route/*.js",
            "angularAMD": "bower_components/angularAMD/*.js",
            "jquery": "bower_components/jquery/dist/*.js",
            "components-font-awesome": [
                "bower_components/components-font-awesome/css/*.css",
                "bower_components/components-font-awesome/fonts/*.*"
            ],
            "bootswatch-dist": [
                "bower_components/bootswatch-dist/css/*.css",
                "bower_components/bootswatch-dist/fonts/*.*",
                "bower_components/bootswatch-dist/js/*.js"
            ],
            "ionicons": [
                "bower_components/Ionicons/css/*.css",
                "bower_components/Ionicons/fonts/*.*"
            ]
        }
    }
}
